{
	"name": "travel-planner",
	"version": "1.0.0",
	"description": "智能旅游规划应用 - 基于阿里通义千问 AI",
	"private": true,
	"scripts": {
		"dev": "concurrently \"pnpm dev:backend\" \"pnpm dev:frontend\"",
		"dev:backend": "pnpm --filter backend dev",
		"dev:frontend": "pnpm --filter frontend dev",
		"build": "pnpm --filter backend build && pnpm --filter frontend build",
		"build:backend": "pnpm --filter backend build",
		"build:frontend": "pnpm --filter frontend build",
		"test": "pnpm --filter backend test && pnpm --filter frontend test",
		"test:backend": "pnpm --filter backend test",
		"test:frontend": "pnpm --filter frontend test"
	},
	"devDependencies": {
		"concurrently": "^8.2.2",
		"typescript": "^5.3.3"
	},
	"keywords": [
		"ai",
		"travel",
		"qwen",
		"nestjs",
		"react"
	],
	"author": "",
	"license": "MIT"
}
